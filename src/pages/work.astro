---
import Layout from "../layouts/Layout.astro";
import ProjectsTable from "../components/ProjectsTable.astro";
import { projects as _projects } from "../data/projects.js";
const projects = _projects.filter((project) =>
  project.collections.includes("portfolio")
);
---

<Layout
  title="Generalist software developer and open source library author - Jonathan Youngblood"
>
  <div class="mt4 mw8-ns w-100 center lh-copy f5 pa0-ns pa3">
    <div class="flex-ns justify-between mb4 pb2">
      <div class="pr6">
        <h2 class="normal">Jonathan Youngblood</h2>

        <div>
          <br />
          <p>Generalist software developer and open source library author.</p>

          <p>
            I love building custom portals and dashboards, APIs, internal tools,
            and of course OSS libraries.
          </p>
          <p>
            In addition to custom development, I consult with companies and
            organizations, helping them achieve success with web sites and
            custom applications, data analysis and reporting, and more.
          </p>

          <br /><br />
          <br /><br />
        </div>

        <h2 class="normal">Preferred Technologies</h2>
        <br />
        Astro, Tailwind, PHP, MySQL, SQLite, Node.js, JavaScript, Bash, Laravel
        <br /><br /><br />
      </div>

      <div>
        <h2 class="normal">&nbsp;</h2>

        <br />

        <a
          class="no-underline"
          href="/Jonathan Youngblood - resume.pdf"
        >
          <span class="flex items-center">
            <svg
              class="mr2 w1"
              viewBox="0 0 24 24"
              fill="currentColor"
              xmlns="http://www.w3.org/2000/svg"
            >
              <path
                d="M20 2H8c-1.1 0-2 .9-2 2v12c0 1.1.9 2 2 2h12c1.1 0 2-.9 2-2V4c0-1.1-.9-2-2-2zm-8.5 7.5c0 .83-.67 1.5-1.5 1.5H9v2H7.5V7H10c.83 0 1.5.67 1.5 1.5v1zm5 2c0 .83-.67 1.5-1.5 1.5h-2.5V7H15c.83 0 1.5.67 1.5 1.5v3zm4-3H19v1h1.5V11H19v2h-1.5V7h3v1.5zM9 9.5h1v-1H9v1zM4 6H2v14c0 1.1.9 2 2 2h14v-2H4V6zm10 5.5h1v-3h-1v3z"
              ></path>
            </svg>
            <span class="underline">Resume</span>
          </span>
        </a>

        <br />

        <a
          class="no-underline"
          target="_blank"
          href="https://www.linkedin.com/in/jonathanyoungblood/"
        >
          <span class="flex items-center">
            <svg
              class="mr2 w1"
              viewBox="0 0 24 24"
              fill="currentColor"
              xmlns="http://www.w3.org/2000/svg"
            >
              <path
                d="M20.447 20.452h-3.554v-5.569c0-1.328-.027-3.037-1.852-3.037-1.853 0-2.136 1.445-2.136 2.939v5.667H9.351V9h3.414v1.561h.046c.477-.9 1.637-1.85 3.37-1.85 3.601 0 4.267 2.37 4.267 5.455v6.286zM5.337 7.433c-1.144 0-2.063-.926-2.063-2.065 0-1.138.92-2.063 2.063-2.063 1.14 0 2.064.925 2.064 2.063 0 1.139-.925 2.065-2.064 2.065zm1.782 13.019H3.555V9h3.564v11.452zM22.225 0H1.771C.792 0 0 .774 0 1.729v20.542C0 23.227.792 24 1.771 24h20.451C23.2 24 24 23.227 24 22.271V1.729C24 .774 23.2 0 22.222 0h.003z"
              ></path>
            </svg>
            <span class="underline">Linkedin</span>
          </span>
        </a>
        <br />
        <a
          class="no-underline"
          target="_blank"
          href="https://github.com/jyoungblood"
        >
          <span class="flex items-center">
            <svg
              class="mr2 w1"
              viewBox="0 0 24 24"
              fill="currentColor"
              xmlns="http://www.w3.org/2000/svg"
            >
              <path
                d="M12 .297c-6.63 0-12 5.373-12 12 0 5.303 3.438 9.8 8.205 11.385.6.113.82-.258.82-.577 0-.285-.01-1.04-.015-2.04-3.338.724-4.042-1.61-4.042-1.61C4.422 18.07 3.633 17.7 3.633 17.7c-1.087-.744.084-.729.084-.729 1.205.084 1.838 1.236 1.838 1.236 1.07 1.835 2.809 1.305 3.495.998.108-.776.417-1.305.76-1.605-2.665-.3-5.466-1.332-5.466-5.93 0-1.31.465-2.38 1.235-3.22-.135-.303-.54-1.523.105-3.176 0 0 1.005-.322 3.3 1.23.96-.267 1.98-.399 3-.405 1.02.006 2.04.138 3 .405 2.28-1.552 3.285-1.23 3.285-1.23.645 1.653.24 2.873.12 3.176.765.84 1.23 1.91 1.23 3.22 0 4.61-2.805 5.625-5.475 5.92.42.36.81 1.096.81 2.22 0 1.606-.015 2.896-.015 3.286 0 .315.21.69.825.57C20.565 22.092 24 17.592 24 12.297c0-6.627-5.373-12-12-12"
              ></path>
            </svg>
            <span class="underline">Github</span>
          </span>
        </a>

        <br />
        <a class="no-underline" href="mailto:jonathan.youngblood@gmail.com">
          <span class="flex items-center">
            <svg
              class="mr2 w1"
              viewBox="0 0 24 24"
              fill="currentColor"
              xmlns="http://www.w3.org/2000/svg"
            >
              <path
                d="M20 4H4c-1.1 0-1.99.9-1.99 2L2 18c0 1.1.9 2 2 2h16c1.1 0 2-.9 2-2V6c0-1.1-.9-2-2-2zm0 4l-8 5-8-5V6l8 5 8-5v2z"
              ></path>
            </svg>
            <span class="underline">Email</span>
          </span>
        </a>

        <br /><br /><br />
      </div>
    </div>

    <br /><br />
    <h2 class="normal">Select Work</h2>
    <br />
    <ProjectsTable projects={projects} />

    <!-- <img src="/images/bio-desk.jpg" /> -->
  </div>
</Layout>
